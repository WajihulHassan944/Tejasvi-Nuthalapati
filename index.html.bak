<script>
   async function fetchImages() {
  try {
    const response = await fetch('https://blogs-admin-editor.vercel.app/blogsFmma');
    const data = await response.json();

    const imagesDiv = document.getElementById('blogList');

    data.forEach(image => {
      const formattedDate = image.blogDate.split('T')[0];

      imagesDiv.innerHTML += `<div class="col-md-4 wow slideInUp" data-wow-delay="0.1s">
                                <div class="blog-item rounded overflow-hidden" style="background:#fef1ef;">
                                    <div class="blog-img position-relative overflow-hidden">
                                      <div style="height:300px; overflow:hidden;">   <a href="detail.html?blogId=${image._id}"> <img class="img-fluid" src="${image.url}" alt="" style="width:100%;cursor:pointer;"></a></div>
                                        <a class="position-absolute top-0 start-0 text-white rounded-end mt-5 py-2 px-4"  style="background:#f9a392;">Tejasvi</a>
                                    </div>
                                    <div class="p-4" style="background:#fef1ef;">
                                        <div class="d-flex mb-3">
                                            <small class="me-3"><i class="far fa-user  me-2" style="color:#f9a392;"></i>Tejasvi Nuthalapati </small>
                                            <small><i class="far fa-calendar-alt me-2" style="color:#f9a392;"></i>${formattedDate}</small>
                                        </div>
                                        <a href="detail.html?blogId=${image._id}"><h4 class="mb-3">${image.title}</h4></a>
                                        <div style="height:50px; overflow:hidden; margin-bottom:15px;">${image.text}</div>
                                        <a class="text-uppercase" href="detail.html?blogId=${image._id}" style="color:#f9a392;">Read More <i class="bi bi-arrow-right"></i></a>
                                    </div>
                                </div>
                            </div>`;
    });

  } catch (error) {
    console.error('Error fetching images:', error);
  }
}

fetchImages();
</script>





